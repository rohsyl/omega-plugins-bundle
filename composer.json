{
    "name": "rohsyl/omega-plugins-bundle",
    "description": "OmegaCMS Plugins Bundle",
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "rohsyl",
            "email": "syzin12@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/auth": "^8.0",
        "illuminate/container": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/database": "^8.0",
        "laravelcollective/html": "^6.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^8.0",
        "rohsyl/omega-core": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "rohsyl\\OmegaPlugin\\Bundle\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "rohsyl\\OmegaPlugin\\Bundle\\ServiceProvider"
            ]
        }
    },

    "repositories": [
        {
            "type": "path",
            "url": "../omega-core"
        }
    ]
}
